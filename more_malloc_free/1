#include "main.h"
#include <stdio.h>
#include <stdlib.h>
#include <limits.h>

/**
 * malloc_checked - function that allocates memory using malloc.
 * @b: parametr
 *
 * Return: co
 */

void *malloc_checked(unsigned int b);
{
	int *co;
	
	co = malloc(b * sizeof(int));
	
	if (co == NULL)
	{
		return (NULL);
	}
	return (co);

}
